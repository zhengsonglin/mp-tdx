<!--pages/component/swiper/index.wxml-->
<swiper indicator-dots="{{indicatorDots}}" indicator-color="{{indicatorColor}}" indicator-active-color="{{indicatorActColor}}" autoplay="{{isAutoplay}}" interval="{{autoplayTime}}" 
    current="{{index}}" circular="{{circular}}" vertical="{{isVertical}}" style="height:{{height}}" bindchange="swiperChange">
	<block wx:for="{{datas}}" wx:key="*this">
		<swiper-item>
			<image src="{{item}}" mode="scaleToFill" class="response" style="height:100%"></image>
		</swiper-item>
	</block>
</swiper>