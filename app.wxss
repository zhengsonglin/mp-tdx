/**app.wxss**/
@import "colorui/main.wxss";
@import "colorui/icon.wxss";
@import "colorui/icon-font.wxss";
/**自定义样式start**/
.text-darkGray{
  color: var(--darkGray)
}
.relative{
  position: relative;
}
.absolute{
  position: absolute;
}
.fullScreenPage {
  height: 100vh;
}
.scrollPage {
  height: 100vh;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/**自定义样式end**/